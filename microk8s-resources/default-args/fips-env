# In a node where FIPS is enabled, ie /proc/sys/crypto/fips_enabled is set to 1
# set the openssl execution environment:
# export OPENSSL_EXECUTABLE="/usr/bin/openssl"
# export OPENSSL_CONF="/etc/ssl/openssl.cnf"

# Set GOFIPS to use FIPS certified libs. If not set GOFIPS is implicetely selected.
export GOFIPS=0
